                <div class="form-signin">
                    <form action="/login" method="post">
                          <h1 class="h3 mb-3 fw-normal" style="margin-left: 38%;">Login</h1>
                      <div class="form-floating">
                        <input name="enrid" class="form-control top" id="enrid" placeholder="Enrollment ID" required autofocus>
                        <label for="floatingInput">Enrollment Id</label>
                      </div>
                      
                      <div class="form-floating">
                        <input type="password" name="password" class="form-control middle" id="password" placeholder="Password" required>
                        <label for="floatingInput">password</label>
                      </div>
                  
                     <br />
                  
                      <button class="w-100 btn btn-lg btn-primary" type="submit">Login</button>
                      <p class="mt-5 mb-3 text-muted">&copy;WITimes</p>
                    </form>
                    <div>
                      <% if(errors != "") { %>
                          <% errors.forEach((item) => { %>
                              <div class="alert alert-danger">
                                  <%- item %>
                              </div>
                          <% }); %>
                      <% } %>
                  </div>
                </div>
